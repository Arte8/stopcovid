<div class="booyah-box col-10 offset-1">

<h1>Enter Data</h1
  <%= simple_form_for @patient do |f| %>
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :phone %>
    <%= f.input :address %>
    <%= f.input :symptoms %>
    <%= f.input :treatment %>
    <%= f.input :travel %>
    <%= f.input :status %>
    <br />
    <%= f.submit 'Create', class: 'btn btn-primary' %>
  <% end %>
</div>
